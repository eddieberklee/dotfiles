export EDITOR=vim

# Quick way to edit and source zshrc
alias ezsh_universal="vi ~/dotfiles/rc/zshrc_universal; /Users/edlee/dotfiles/rc/copy-zshrc.sh; . ~/.zshrc; ~/dotfiles/push.sh"
alias szsh_universal=". ~/dotfiles/rc/zshrc_universal"
alias ezsh_facebook="vi ~/dotfiles/rc/zshrc_facebook; /Users/edlee/dotfiles/rc/copy-zshrc.sh; . ~/.zshrc; ~/dotfiles/push.sh"
alias szsh_facebook=". ~/dotfiles/rc/zshrc_facebook"

# Typo catcher
alias sl="ls"

# Android
alias adb_restart="adb kill-server; adb start-server"



# Autosuggestions https://github.com/zsh-users/zsh-autosuggestions
# `brew install zsh-autosuggestions`
source /usr/local/share/zsh-autosuggestions/zsh-autosuggestions.zsh



